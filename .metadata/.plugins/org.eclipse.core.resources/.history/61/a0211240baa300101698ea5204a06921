#include "ComunicationLib.hpp"



MyUSART::MyUSART (uint8_t Header, uint8_t ClosingBit){
	header = Header;
	closing_bit = ClosingBit;
	for(int i=0; i<BufferLen; i++){
		rx_buffer[i]=0x00;
	}
}

MyUSART Uart1(0xAA, 0xEE);
